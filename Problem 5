#include <iostream>
#include <conio.h>

using namespace std;

int main ()
{ int i=1;
  
  cout << "This Program Counts Up to Ten by ones then by twos to Thirty \n" ;
  cout << "\n";
  
  while (i <= 10)
  { cout << i << ",";
  	i++;
  }

  for (i=12; i <= 30; i++)
   {
  if (i==13) continue;
  if (i==15) continue;
  if (i==17) continue;
  if (i==19) continue;
  if (i==21) continue;
  if (i==23) continue;
  if (i==25) continue;
  if (i==27) continue;
  if (i==29) continue;
  if (i==30) {cout << i; continue;}
    cout << i << ",";
   }
   
  cout << "30";
  
  
  _getch();
  return 0;
}
